{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "!/*-es5*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/s/**"
        ]
      }
    }, {
      "name": "assets-eager",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/*-es5*.js",
          "/assets/icons/*",
          "/assets/images/*"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "dark-sky-api",
      "urls": [
        "https://cors-anywhere.herokuapp.com/https://api.darksky.net/forecast/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "12h",
        "timeout": "3s",
        "strategy": "freshness"
      }
    }
  ]
}
